<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.com.marcing.android.dynamicactivityloader" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DynamicClassLoadActivity"
            android:label="@string/title_activity_dynamic_class_load" >
        </activity>
        <activity android:name="pl.com.marcing.android.customdex.TestActivity" >
        </activity>
        <activity
            android:name=".DynamicActivityLoadActivity"
            android:label="@string/title_activity_dynamic_activity_load" >
        </activity>
    </application>

</manifest>
